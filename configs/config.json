{
    "name": "final",
    "n_gpu": 1,
    "preprocessing": {
      "sr": 22050,
      "spectrogram": {
        "type": "MelSpectrogram",
        "args": {
        }
      },
      "log_spec": false
    },
    "augmentations": {
      "wave": [],
      "spectrogram": []
    },
    "arch": {
      "type": "HiFiGAN",
      "args": {
        "Generator": {
          "n_mels": 80,
          "h_init": 256,
          "ku": [16, 16, 8],
          "kr": [3, 5, 7],
          "dilations": [[[1], [2]], [[2], [6]], [[3], [12]]]
        },
        "MPD": {
          "ps":  [2, 3, 5, 7, 11]
        }
      }
    },
    "data": {
      "train": {
        "num_workers": 8,
        "batch_size": 16,
        "datasets": [
          {
            "type": "LJspeechDataset",
            "args": {
              "part": "train",
              "data_dir": "/kaggle/input/ljspeech/data/datasets/ljspeech",
              "max_audio_length": 20.0,
              "limit": 16,
              "train": true
            }
          }
        ]
      },
      "val": {
        "num_workers": 8,
        "batch_size": 16,
        "datasets": [
          {
            "type": "LJspeechDataset",
            "args": {
              "part": "test",
              "data_dir": "/kaggle/input/ljspeech/data/datasets/ljspeech",
              "max_audio_length": 20.0,
              "limit": 16,
              "train": false
            }
          }
        ]
      }
    },
    "optimizer": {
      "Generator": {
        "type": "Adam",
        "args": {
          "lr": 3e-4
        }
      },
      "Discriminator": {
        "type": "Adam",
        "args": {
          "lr": 3e-4
        }
      }

    },
    "loss": {
      "type": "GeneratedLoss",
      "args": {}
    },
    "lr_scheduler": {
      "Generator": {
        "type": "OneCycleLR",
        "args": {
          "steps_per_epoch": 100,
          "epochs": 5,
          "anneal_strategy": "cos",
          "max_lr": 3e-4,
          "pct_start": 0.2
        }
      },
      "Discriminator": {
        "type": "OneCycleLR",
        "args": {
          "steps_per_epoch": 100,
          "epochs": 5,
          "anneal_strategy": "cos",
          "max_lr": 3e-4,
          "pct_start": 0.2
        }
      }

    },
    "trainer": {
      "epochs": 5,
      "save_dir": "saved/",
      "save_period": 5,
      "verbosity": 2,
      "monitor": "min val_loss",
      "visualize": "wandb",
      "wandb_project": "HiFiGAN",
      "len_epoch": 100,
      "grad_norm_clip": 10
    }
  }
  